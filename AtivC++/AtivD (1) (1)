#include <iostream>

int main() {
    double num1, num2, resultado;
    std::cout << "Digite o primeiro número: ";
    std::cin >> num1;
    std::cout << "Digite o segundo número: ";
    std::cin >> num2;
    if (num2 != 0) {
      
        resultado = num1 / num2;
        std::cout << "O resultado da divisão de " << num1 << " por " << num2 << " é " << resultado << std::endl;
    } else {
        std::cout << "Erro: Divisão por zero não é permitida." << std::endl;
    }

    return 0;
}
