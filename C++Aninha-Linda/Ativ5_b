#include <iostream>
using namespace std;

int main() {
    int numero;
    cout << "Digite um número inteiro positivo: ";
    cin >> numero;
    if (numero < 0) {
        cout << "Por favor, insira um número positivo." << endl;
        return 1;
    }
    cout << "Números pares entre 0 e " << numero << endl;

    for (int i = 0; i <= numero; i += 2) {
        cout << i << " ";
    }

    cout << endl;
    return 0;
}